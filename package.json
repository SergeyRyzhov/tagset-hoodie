{
  "name": "tagset",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "npm run test:lint && npm run test:karma",
    "test:watch": "npm run test:lint && npm run test:karma:watch",
    "test:lint": "vue-cli-service lint",
    "test:karma": "karma start test/karma.config.js --singleRun",
    "test:karma:watch": "karma start test/karma.config.js",
    "start": "hoodie",
    "start2": "node \"./node_modules/hoodie/bin/start.js\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.0.0-rc.2",
    "clipboard-polyfill": "^2.4.6",
    "hoodie": "^28.2.6",
    "jquery": "^1.9.1",
    "lodash": "^4.17.5",
    "octicons": "^7.2.0",
    "pouchdb": "^6.4.3",
    "vue": "^2.5.13",
    "vue-hoodie": "git+https://github.com/SergeyRyzhov/vue-hoodie.git",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "jasmine": "^3.1.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.0",
    "offline-plugin": "^4.9.0",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
